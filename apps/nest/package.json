{
  "name": "nest",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "code:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts:check": "tsc",
    "lint": "yarn code:check && yarn ts:check",
    "setup": "yarn install && yarn database:migration:run",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "database:migration:create": "bash ./scripts/create-migration.sh",
    "database:migration:run": "bash ./scripts/run-migrations.sh postgres.ts",
    "database:migration:revert": "bash ./scripts/revert-migrations.sh postgres.ts",
    "database:entity:create": "bash ./scripts/create-entity.sh",
    "database:subscriber:create": "bash ./scripts/create-subscriber.sh"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.505.0",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "amazon-cognito-identity-js": "^6.3.7",
    "axios": "^1.6.7",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jwks-rsa": "^3.1.0",
    "nestjs-twilio": "^4.3.0",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pug": "^3.0.2",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "serialize-interceptor": "^1.1.6",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "~types": "*"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/testing": "^10.3.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nock": "^13.5.1",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "testing": "*",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}